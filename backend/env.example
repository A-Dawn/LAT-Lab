# 基础配置
DEBUG=False  # 生产环境务必设置为False以隐藏错误详情和堆栈跟踪
HOST=0.0.0.0
PORT=8000

# 数据库设置
DB_TYPE=sqlite  # sqlite或mysql
MYSQL_USER=lat_lab_user
MYSQL_PASSWORD=your_db_password
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DB=lat_lab_db

# JWT设置
SECRET_KEY=your_secret_key_here  # 请将此项更改为随机密钥

# 邮件设置
MAIL_SERVER=smtp.example.com
MAIL_PORT=587
MAIL_USERNAME=your-email@example.com
MAIL_PASSWORD=your-email-password
MAIL_FROM=your-email@example.com
MAIL_TLS=True
MAIL_SSL=False

# 网站基础URL
BASE_URL=http://localhost:5173

# 速率限制配置
    RATE_LIMIT_ENABLED: bool = True
    RATE_LIMIT_LOGIN_REQUESTS: int = 20   # 登录每分钟最多20次
    RATE_LIMIT_LOGIN_WINDOW: int = 60     # 时间窗口60秒
    RATE_LIMIT_API_REQUESTS: int = 200   # API每分钟最多200次
    RATE_LIMIT_API_WINDOW: int = 60       # 时间窗口60秒
    RATE_LIMIT_UPLOAD_REQUESTS: int = 20  # 上传每分钟最多20次
    RATE_LIMIT_UPLOAD_WINDOW: int = 60    # 时间窗口60秒
    RATE_LIMIT_PLUGIN_REQUESTS: int = 100 # 插件运行每分钟最多100次
    RATE_LIMIT_PLUGIN_WINDOW: int = 60    # 时间窗口60秒

# 插件市场配置
PLUGIN_MARKETPLACE_SOURCE=local
PLUGIN_MARKETPLACE_GIT_REPO=
PLUGIN_MARKETPLACE_GIT_BRANCH=main
PLUGIN_MARKETPLACE_GIT_PATH=marketplace_config.json
PLUGIN_MARKETPLACE_GIT_TOKEN= 